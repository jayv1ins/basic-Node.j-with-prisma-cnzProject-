<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
</head>
<body>
  <h1>User View</h1>
  <table>
    <tr>
      <th>ID</th>
      <td><%= user.id %></td>
    </tr>
    <tr>
      <th>Username</th>
      <td><%= user.username %></td>
    </tr>
    <tr>
      <th>Email</th>
      <td><%= user.email %></td>
    </tr>
    <tr>
      <th>User Type</th>
      <td><%= user.usertype %></td>
    </tr>
    <tr>
      <th>Shift</th>
      <td><%= user.shift %></td>
    </tr>
    <tr>
      <th>Created At</th>
      <td><%= user.createdAt %></td>
    </tr>
    <tr>
      <th>Updated At</th>
      <td><%= user.updatedAt %></td>
    </tr>
  </table>
</body>
</html>
